{%- comment -%} as_canonical_the_page_url.liquid {%- endcomment -%}
{%- assign _site_name = site.name -%}
{%- assign _page_url = 'null' -%}

{%- capture _page_url -%}
    {%- include as_canonical_url_from_url.liquid url=page.url -%}
{%- endcapture -%}

{%- comment -%} return {%- endcomment -%}
{%- assign _return = _page_url | upcase -%}
{%- case _return -%}
    {%- when "" or "NULL" or null -%}
        {{ null }}
    {%- else -%}
        {{ _page_url }}
{%- endcase -%}
{%- assign _return = null -%}

{%- assign _site_name = null -%}
{%- assign _page_url = null -%}

{%- comment -%} {%- endcomment -%}