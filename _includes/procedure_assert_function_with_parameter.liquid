{%- assign function = include.function -%}
{%- assign _parameter = include.parameter -%}
{%- assign _expected = include.expected -%}
{%- assign _with_name = include.with_name -%}
{%- assign _with_version = include.with_version -%}
{%- assign _with_success = include.with_success -%}
{%- assign _with_warning = include.with_warning -%}

{%- capture _result -%}
    {% include {{ function }} parameter=_parameter %}
{%- endcapture -%}

{%- capture assert -%}
    {% include procedure_assert_this_and_that.liquid
            this=_expected
            that=_result
            with_name=_with_name
            with_version=_with_version
            with_success=_with_success
            with_warning=_with_warning
    %}
{%- endcapture -%}
{{ assert }}

{%- assign function = null -%}
{%- assign _parameter = null -%}
{%- assign _expected = null -%}
{%- assign _with_name = null -%}
{%- assign _with_version = null -%}
{%- assign _with_success = null -%}
{%- assign _with_warning = null -%}